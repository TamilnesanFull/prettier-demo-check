{
    "name": "react-performance",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "rsbuild dev --open",
        "build": "rsbuild build",
        "preview": "rsbuild preview",
        "format-all": "npx prettier . --write",
        "format-check": "npx prettier . --check",
        "prepare": "husky install"
    },
    "dependencies": {
        "husky": "^9.0.11",
        "million": "^3.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@rsbuild/core": "^0.4.7",
        "@rsbuild/plugin-react": "^0.4.7",
        "@types/react": "^18.2.47",
        "@types/react-dom": "^18.2.18",
        "prettier": "3.2.5",
        "typescript": "^5.3.0"
    },
    "lint-staged": {
        "*.{ts,tsx,css,scss,html,json,md}": [
            "prettier --write --config=./.prettierrc --no-editorconfig"
        ]
    }
}
